<h1>New multiplication</h1>

<form action="/wizard_multiply">
  <label for="dividend">
    Multiply this:
  </label>
  <input id="dividend" name="first_number" text="text">

  <label for="divisor">
    by this:
  </label>
  <input id="divisor" name="second_num" text="text">

  <button>
    Multiply!
  </button>
</form>
